@model List<Goblin.Models.User>
<!DOCTYPE html>
<html>

<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>Goblin</title>
    <link href="~/styles/bootstrap.min.css" rel="stylesheet">
</head>
<body>
<div class="container-fluid">
    <h1>Японский гоблин</h1>
    <ul class="nav nav-pills mb-3" id="pills-tab" role="tablist">
        <li class="nav-item">
            <a class="nav-link active" id="pills-home-tab" data-toggle="pill" href="#pills-home" role="tab">Главная</a>
        </li>
        <li class="nav-item">
            <a class="nav-link" id="pills-messages-tab" data-toggle="pill" href="#pills-messages" role="tab">Сообщения</a>
        </li>
        <li class="nav-item">
            <a class="nav-link" href='@Url.Action("Logout", "Admin")'>Выход</a>
        </li>
    </ul>

    <div class="tab-content" id="pills-tabContent">
        <div class="tab-pane fade show active" id="pills-home" role="tabpanel">
            @await Html.PartialAsync("_UsersTable", Model)
        </div>
        <div class="tab-pane fade" id="pills-messages" role="tabpanel">
            @await Html.PartialAsync("_MessagesPart")
        </div>
    </div>
</div>
<script src="js/jquery-3.1.1.min.js"></script>
<script src="js/popper.min.js"></script>
<script src="js/bootstrap.min.js"></script>
<script src="js/main.min.js"></script>
</body>
</html>